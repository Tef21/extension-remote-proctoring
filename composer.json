{
	"name": "oat-sa/extension-remote-proctoring",
	"type": "tao-extension",
	"description": "tao extension to allow you to proctor your delivery executions by a remote proctor",
	"authors": [
		{
			"name": "Open Assessment Technologies SA"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:oat-sa/lib-proctorio.git"
		}
	],
	"keywords": [
		"tao",
		"computer-based-assessment"
	],
	"homepage": "http://www.taotesting.com",
	"license": "GPL-2.0-only",
	"extra": {
		"tao-extension-name": "remoteProctoring"
	},
	"minimum-stability": "dev",
	"require": {
		"oat-sa/oatbox-extension-installer" : "~1.1||dev-master",
		"oat-sa/lib-proctorio": "~1.0",
		"generis" : "*"
  },
	"autoload" : {
		"psr-4" : {
			"oat\\remoteProctoring\\" : ""
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5"
	},
	"autoload-dev": {
		"psr-4": {
			"oat\\Proctorio\\tests\\": "test"
		}
	}
}
